{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "*.{jpg,gif,png,css,scss,js,ico,svg}",
            "assets/*"
        ]
    },
    "routes": [
        {
            "route": "/.auth/login/twitter",
            "statusCode": 404
        },
        {
            "route": "/api/*",
            "allowedRoles": ["authenticated"]
        }
    ]
}